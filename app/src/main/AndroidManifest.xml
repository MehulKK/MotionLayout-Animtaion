<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mehulk.motionlayout">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".DashBoardActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ConstraintSetActivity"
                  android:label="With Constraint Set"
                  android:screenOrientation="portrait"/>

        <activity android:name=".SimpleActivity"
                  android:label="Simple"
                  android:screenOrientation="portrait"/>

        <activity android:name=".KeyFrameSetActivity"
                  android:label="KeyFrame Set"
                  android:screenOrientation="portrait"/>

        <activity android:name=".ParallaxActivity"
                  android:label="Parallax"
                  android:screenOrientation="portrait"/>
    </application>

</manifest>